<project xmlns="http://maven.apache.org/POM/4.0.0"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
 <modelVersion>4.0.0</modelVersion>
  <groupId>com.nativelibs4java</groupId>
  <artifactId>nl4j-runtime-structs</artifactId>
  <name>NativeLibs4Java Structs</name>
  <url>http://code.google.com/p/nativelibs4java/wiki/Runtime</url>
  <version>${nl4j-runtime.version}</version>
  <packaging>jar</packaging>

  <parent>
	  <groupId>com.nativelibs4java</groupId>
	  <artifactId>nl4j-runtime-parent</artifactId>
	  <version>1.0-SNAPSHOT</version>
	  <relativePath>../Parent</relativePath>
  </parent>

  <dependencies>
  	
	<dependency>
	  <groupId>com.jnaerator</groupId>
	  <artifactId>jna-jnaerator</artifactId>
	  <version>${jna.version}</version>
	  <scope>compile</scope>
	</dependency>
	
	<!--dependency>
	  <groupId>com.jnaerator</groupId>
	  <artifactId>jnaerator-runtime</artifactId>
	  <version>${jnaerator.version}</version>
	  <scope>compile</scope>
	</dependency-->
	
  </dependencies>

  <build>
    <plugins>
	
		<plugin>
			<groupId>com.nativelibs4java</groupId>
			<artifactId>maven-velocity-plugin</artifactId>
			<version>${nl4j-runtime.version}</version>
			<executions>
			  <execution>
				<phase>generate-sources</phase>
				<goals>
				  <goal>compile</goal>
				</goals>
			  </execution>
			</executions>
		</plugin>
	
	</plugins>
  </build>
</project>



